#include<bits/stdc++.h>
using namespace std;

int main(){
  int T,l;
  string s;
   cin>>T;
   while(T--){
        cin>>s;
       l=s.length();

       if(s=="1" || s=="4" || s=="78")
            cout<<"+"<<endl;
       else if(s[l-2]=='3' && s[l-1]=='5')
             cout<<"-"<<endl;
      else if(s[0]=='1' && s[1]=='9' && s[2]=='0')
         cout<<"?"<<endl;
       else if(s[0]=='9' && s[l-1]=='4')
         cout<<"*"<<endl;
         else
               cout<<"+"<<endl;
   }

 return 0;
}
