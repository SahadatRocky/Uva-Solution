#include<bits/stdc++.h>
using namespace std;
int main(){

  int n;
  string s="one";
    cin>>n;
    int i=0;
    while(i<n){
        string str;
            cin>>str;

         if(str.length()==5){
            cout<<"3"<<endl;
         }
         else{
             int cnt=0;
               for(int i=0;i<3;i++){

                   if(str[i]==s[i]){
                    cnt++;
                   }
               }
               (cnt>=2) ? cout<<"1"<<endl : cout<<"2"<<endl;
         }
        i++;
    }
 return 0;
}
