#include<bits/stdc++.h>
using namespace std;

int main(){
	   int h1,m1,h2,m2;
	   int f,s;
	   while(scanf("%d %d %d %d",&h1,&m1,&h2,&m2)==4){
	   	if(h1==0 && m1==0 && h2==0 && m2==0){
	   		break;
	   }
	   else{
	   	f=(h1*60)+m1;
	   	s=(h2*60)+m2;
	   	  if(s<f)
	   	  {
	   	  	s=(24*60)+s;
	   	  	}
	   	cout<<(s-f)<<endl;
           }
	   	
	   	}
	  
 	 
   return 0;
    }


