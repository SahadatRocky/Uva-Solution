#include<bits/stdc++.h>
using namespace std;


int main(){
      
     int k,n=1; 
     int c[12],r[12];
     
     while(scanf("%d",&k) && k>=0){
     	
     	for(int i=0;i<12;i++){
     		  cin>>c[i];
     		  }
     		  for(int j=0;j<12;j++){
     		
     		     cin>>r[j];
     		     
     	}
     	  cout<<"Case "<<n<<":"<<endl;
     	    
     	    for(int i=0;i<12;i++){
     	    	   
     	    	   
     	    	   if(k<r[i]){
     	    	   	cout<<"No problem. :("<<endl;
     	    	   	
     	    	   	}
     	    	   	else{
     	    	   		cout<<"No problem! :D"<<endl;
     	    	   		
     	    	   		k=k-r[i];
     	    	   		
     	    	   		}
     	    	
     	    	    k+=c[i];
     	    	
     	    	}
     	
     	
     	   n++;
     	}
     

   return 0;
}
