#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,m,res;
    while(1)
    {
        cin>>n>>m;

        if(n<0 && m<0)
            break;
        if(n>m)
        {
           res =n-m;
        }
        else
        {
            res=m-n;
        }

        if(res>=50){
            res = 100-res;
            }
           cout<<res<<endl;
    }
return 0;
}
